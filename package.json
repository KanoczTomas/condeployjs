    "deploy",
    "snmp"
  ],
  "author": "Tomas Kanocz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KanoczTomas/condeployjs/issues"
  },
  "homepage": "https://github.com/KanoczTomas/condeployjs#readme",
  "dependencies": {
    "body": "^5.1.0",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "snmp-native": "^1.0.21",
    "uglify-js": "^2.6.4"
  },
  "devDependencies": {
    "browserify": "^13.0.1",
    "debowerify": "^1.5.0",
    "morgan": "^1.7.0",
    "should": "^9.0.2",
    "uglify": "^0.1.5",
    "watchify": "^3.7.0"
  },
  "browserify": {
